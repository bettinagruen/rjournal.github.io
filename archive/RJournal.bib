{% assign lbrace = '{' %}
{% assign rbrace = '}' %}

{% for issue in site.issues %}{% for article in issue.articles %}{% if article.slug %}

{% if issue.volume < 6 %}
@Article{ RJournal:{{issue.year}}-{{issue.num}}:{{article.slug}},
{% if article.bibauthor %}  author       = {{lbrace}}{{article.bibauthor}}{{rbrace}}, {% else %}  author       = {{lbrace}}{{article.author | join: " and "}}{{rbrace}},{% endif %}
  title        = {{lbrace}}{{lbrace}}{{article.title}}{{rbrace}}{{rbrace}},
  journal      = {The R Journal},
  year         = {{issue.year}},
  volume       = {{issue.volume}},
  number       = {{issue.num}},
  pages        = {{lbrace}}{{article.pages | join: "--"}}{{rbrace}},
  month        = {{lbrace}}{{issue.month}}{{rbrace}},
  url          = {http://journal.r-project.org/archive/{{issue.year}}-{{issue.num}}/RJournal_{{issue.year}}-{{issue.num}}_{{article.slug}}.pdf}
}
{% else %}
@Article{ {{article.slug}}:{{issue.year}},
  author       = { {{article.author | join: " and "}} },
  title        = { {{article.title}} },
  journal      = { The R Journal },
  year         = { {{issue.year}} },
  volume       = { {{issue.volume}} },
  number       = { {{issue.num}} },
  pages        = { {{article.pages | join: "--"}} },
  month        = {{issue.month}},
  url          = { http://journal.r-project.org/archive/{{issue.year}}-{{issue.num}}/{{article.slug}}.pdf }
{% endif %}{% endif %}{% endfor %}{% endfor %}
